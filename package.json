{
    "name": "bsafe",
    "version": "0.0.0",
    "description": "A multi-signature system sdk built on Fuel",
    "author": "InfinityBase - Guilherme Roque",
    "license": "Apache-2.0",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rm -rf dist/ && prettier --write src/ && npm run build:esm && npm run build:cjs",
        "build:esm": "tsc",
        "build:cjs": "tsc --module CommonJS --outDir dist/cjs"
    },
    "devDependencies": {
        "@types/node": "17.0.1",
        "@types/react": "17.0.37",
        "@types/react-dom": "17.0.11",
        "prettier": "3.0.3",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "typescript": "4.5.4",
        "turbo": "1.10.0"
    },
    "peerDependencies": {
        "react": "17.0.2",
        "react-dom": "17.0.2"
    }
}
