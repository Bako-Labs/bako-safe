{
  "name": "bakosafe",
  "version": "0.0.65",
  "author": "Infinybase - Guilherme Roque",
  "description": "A signature validation package built based on sway in the fuel network",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "postinstall": "pnpm fuels build",
    "build": "tsup",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand",
    "test:watch:all": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand --verbose --watch",
    "test:name": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand -t"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/elliptic": "^6.4.14",
    "@types/jest": "29.5.4",
    "@types/mocha": "10.0.1",
    "@types/node": "^16.18.11",
    "@types/ramda": "^0.30.0",
    "@types/uuid": "^9.0.6",
    "dotenv": "^16.4.1",
    "fuels": "0.94.3",
    "jest": "29.6.4",
    "patch-package": "8.0.0",
    "prettier": "3.0.3",
    "ts-jest": "29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "~5.4.5"
  },
  "dependencies": {
    "@noble/curves": "^1.5.0",
    "axios": "^1.5.1",
