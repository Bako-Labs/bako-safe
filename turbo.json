{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "prebuild": {
      "dependsOn": [
        "^prebuild"
      ],
      "outputs": [
        "dist/sway/**"
      ],
      "cache": false,
      "outputLogs": "hash-only"
    },
    "build": {
      "outputs": [
        "dist/**"
      ],
      "cache": false,
      "outputLogs": "new-only"
    }
  }
}
