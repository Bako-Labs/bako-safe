{
    "$schema": "https://turbo.build/schema.json",
    "globalEnv": ["NODE_ENV"],
    "pipeline": {
        "build": {
            "dependsOn": ["^build:sway"],
            "outputs": ["dist/**"]
        },
        "build:sway": {
            "cache": true,
            "outputs": ["dist/**"]
        },
        "dev": {
            "dependsOn": ["^build:sway"],
            "cache": false
        }
    }
}
